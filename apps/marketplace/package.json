{
  "name": "marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001 --turbopack",
    "dev:docker": "docker-compose up -d && next dev --port 3001 --turbopack",
    "build": "next build",
    "start": "next start --port 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "localstack:up": "docker-compose up -d localstack",
    "localstack:down": "docker-compose down",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.700.0",
    "@graphql-tools/delegate": "^10.2.9",
    "@graphql-tools/executor-http": "^1.2.2",
    "@graphql-tools/schema": "^10.0.16",
    "@graphql-tools/stitch": "^9.4.11",
    "@graphql-tools/wrap": "^10.0.27",
    "@hookform/resolvers": "^3.9.1",
    "@nimara/codegen": "workspace:*",
    "@nimara/config": "workspace:*",
    "@nimara/domain": "workspace:*",
    "@nimara/infrastructure": "workspace:*",
    "@nimara/ui": "workspace:*",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "clsx": "^2.1.1",
    "@graphql-typed-document-node/core": "3.2.0",
    "graphql": "^16.10.0",
    "graphql-request": "^7.3.1",
    "graphql-yoga": "^5.12.2",
    "jose": "^6.1.0",
    "lucide-react": "^0.544.0",
    "next": "^15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.66.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/near-operation-file-preset": "^3.1.0",
    "@graphql-codegen/typed-document-node": "^5.1.2",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-next": "^15.3.3",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "tsconfig": "workspace:*",
    "typescript": "^5.7.0",
    "vitest": "^3.2.0"
  }
}
